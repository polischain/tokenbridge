  "source": "pragma solidity 0.7.5;\n\nimport \"../interfaces/IERC20Receiver.sol\";\n\ncontract TokenReceiver is IERC20Receiver {\n    address public token;\n    address public from;\n    uint256 public value;\n    bytes public data;\n\n    function onTokenBridged(\n        address _token,\n        uint256 _value,\n        bytes memory _data\n    ) external override {\n        token = _token;\n        from = msg.sender;\n        value = _value;\n        data = _data;\n    }\n}\n",
